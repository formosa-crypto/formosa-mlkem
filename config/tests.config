[default]
bin  = easycrypt
args = -timeout 200

[test-mlkem]
okdirs  = !proof/correctness768 !proof/correctness1024  !proof/eclib  !proof/security
exclude = !proof/attic    !proof/spec 

[test-mlkem_768_avx2]
okdirs  = !proof/correctness768/avx2 
exclude = !proof/attic    !proof/spec !proof/correctness1024  !proof/eclib  !proof/security

[test-mlkem_768_ref]
okdirs  = proof/correctness768
exclude = !proof/attic    !proof/spec !proof/correctness1024  !proof/eclib  !proof/security

[test-mlkem_1024_avx2]
okdirs  = !proof/correctness1024/avx2
exclude = !proof/attic    !proof/spec !proof/correctness768  !proof/eclib  !proof/security

[test-mlkem_1024_ref]
okdirs  = proof/correctness1024
exclude = !proof/attic    !proof/spec !proof/correctness768  !proof/eclib  !proof/security

[test-mlkem_other]
okdirs  = !proof/eclib  !proof/security
exclude = !proof/attic    !proof/spec !proof/correctness768  !!proof/correctness1024