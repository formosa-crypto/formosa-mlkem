fn _shake128_squeezeblock
( reg mut ptr u64[25] st
, reg mut ptr u8[R168] out
) -> reg ptr u64[25], reg ptr u8[R168]
{ 
  st = _keccakf1600_ref(st);
  out, _ = A168::__dumpstate_array_ref(out, 0, R168, st); 
 return st, out;
}

