fn _shake256_128_33
( reg mut ptr u8[128] out
, reg const ptr u8[33] in
) -> reg ptr u8[128]
{ stack u64[25] st_s;
  reg ptr u64[25] st;

  st = st_s;
  st = __state_init_ref(st);
  st, _, _ = A33::__absorb_array_ref(st, 0, in, 0, 33, R136, SHAKE);
  out, _, _ = A128::__squeeze_array_ref(out, 0, 128, st, R136);

  return out;
}

fn _shake256_A32__A1120
( reg mut ptr u8[32] out
, reg const ptr u8[32] in0
, reg const ptr u8[1088] in1) -> reg ptr u8[32]
{ stack u64[25] st_s;
  reg ptr u64[25] st;

  st = st_s;
  st = __state_init_ref(st);
  st, _, _ = A32::__absorb_array_ref(st, 0, in0, 0, 32, R136, UNFINISHED);
  st, _, _ = A1088::__absorb_array_ref(st, 32, in1, 0, 1088, R136, SHAKE);
  out,_, _ = A32::__squeeze_array_ref(out, 0,32, st, R136);
  return out;
}

fn _shake256_A32__A1600
( #spill_to_mmx reg mut ptr u8[32] out
, reg const ptr u8[32] in0
, reg const ptr u8[1568] in1) -> reg ptr u8[32]
{ stack u64[25] st_s;
  reg ptr u64[25] st;

  () = #spill(out);
  () = #spill(in1);
  st = st_s;
  st = __state_init_ref(st);
  st, _, _ = A32::__absorb_array_ref(st, 0, in0, 0, 32, R136, UNFINISHED);
  () = #unspill(in1);
  st, _, _ = A1568::__absorb_array_ref(st, 32, in1, 0, 1568, R136, SHAKE);
  () = #unspill(out);
  out,_, _ = A32::__squeeze_array_ref(out, 0,32, st, R136);
  return out;
}

