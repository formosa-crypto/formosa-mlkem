fn _shake128_absorb34
( reg mut ptr u64[25] st
, reg const ptr u8[34] in
) -> reg ptr u64[25]
{ reg u64 offset;

  st = __state_init_ref(st);
  offset = 0;
  st, _, _ = A34::__absorb_array_ref(st, 0, in, offset, 34, R168, SHAKE);
  
  return st;
