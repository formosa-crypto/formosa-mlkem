fn _shake128_squeezeblock
( reg mut ptr u64[25] st
, #spill_to_mmx reg mut ptr u8[R168] out
) -> reg ptr u64[25], reg ptr u8[R168]
{ reg u64 offset;

  () = #spill(out);
  st = _keccakf1600_ref(st);
  () = #unspill(out);
  offset = 0;
  out, _ = A168::__dumpstate_array_ref(out, offset, R168, st);

  return st, out;
}

